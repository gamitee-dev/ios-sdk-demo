        },
        {
          "inlineContent" : [
            {
              "inlineContent" : [
                {
                  "text" : "You will not be able to interact with JoynedMobileSDK without implementing this protocol.",
                  "type" : "text"
                }
              ],
              "type" : "emphasis"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "@objc(JoynedSDKDelegate)",
            "public protocol JoynedSDKDelegate : NSObjectProtocol",
            "{",
            "    \/\/\/ Called when the SDK was activated successfully",
            "    @objc optional func mobileSDKDidActivateSuccessfully()",
            "    ",
            "    \/\/\/ Called when the SDK failed to activate",
            "    @objc optional func mobileSDKDidFailToActivate()",
            "    ",
            "    \/\/\/ An intervention location allowing to override the locale to be used when presenting SDK content on screen",
            "    @objc optional func mobileSDKLocaleWhenPresentingContent(given computedLocale: String) -> String",
            "    ",
            "    \/\/\/ Called when a user clicks on an offering within the Joyned UI",
            "    @objc func mobileSDKDidTriggerNavigation(for navigationPayload: JoynedNavigationProtocol)",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "inlineContent" : [
                {
                  "text" : "Usage Example:",
                  "type" : "text"
                }
              ],
              "type" : "strong"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "import JoynedMobileSDK",
            "extension AppDelegate : JoynedSDKDelegate",
            "{",
            "    func mobileSDKDidActivateSuccessfully()",
            "    {",
            "        print(\"Called mobileSDKDidActivateSuccessfullyl\")",
            "    }",
            "    func mobileSDKDidFailToActivate()",
            "    {",
            "        print(\"Called mobileSDKDidFailToActivate\")",
            "    }",
            "    func mobileSDKLocaleWhenPresentingContent(given computedLocale: String) -> String ",
            "    {",
            "        print(\"Called mobileSDKLocaleWhenPresentingContent(given:)\")",
            "        return computedLocale.replacingOccurrences(of: \"_\", with: \"-\")",
            "    }",
            "    func mobileSDKDidTriggerNavigation(for navigationPayload: JoynedNavigationProtocol)",
            "    {",
            "        print(\"Called mobileSDKDidTriggerNavigation(for:)\")",
            "        \/\/ Dd in-app navigation",
            "    }",
            "}"
          ],
